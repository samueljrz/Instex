{
	"name": "Instex",
	"version": "1.0",
	"manifest_version": 2,
	"author": "Samuel Junior - https://github.com/samueljrz",
	"description": "Help U to doing download of contents on Instagram",
	"offline_enabled": false,
	"background": {
			"scripts": ["scripts/background.js"],
			"persistent": false
	},
	"browser_action": {
      "default_title": "Instex",
      "default_popup": "content/popup.html",
      "default_icon": {
        "16": "images/instex16.png",
        "32": "images/instex32.png",
        "48": "images/instex48.png",
        "128": "images/instex128.png"
      }
    },
    "icons": {
      "16": "images/instex16.png",
      "32": "images/instex32.png",
      "48": "images/instex48.png",
      "128": "images/instex128.png"
    },
    "permissions": [
      "webRequest"
    ]
}